# EditorConfig is awesome: https://EditorConfig.org

root = true

[*]
charset                  = utf-8
ident_style              = space
ident_size               = 2
insert_final_newline     = true
trim_trailing_whitespace = true

[*.js]
quote_type = single

[*.md]
max_line_length = off
trim_trailing_whitespace = false
